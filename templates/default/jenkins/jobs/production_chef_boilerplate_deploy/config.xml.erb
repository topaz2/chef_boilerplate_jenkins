  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <axes/>
  <builders>
    <hudson.tasks.Shell>
      <command>/usr/local/bin/tools/build/jenkins/chef_boilerplate_deploy/pre.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/usr/local/bin/tools/build/jenkins/chef_boilerplate_deploy/main.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/usr/local/bin/tools/build/jenkins/chef_boilerplate_deploy/post.sh</command>
